<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- 填充的颜色 -->
    <solid android:color="@color/color_000000" />

    <!-- 边框的颜色和粗细 -->
    <stroke
        android:width="1dp"
        android:color="@color/color_FF1B56"
        />

    <!-- android:radius 圆角的半径 -->
    <corners
        android:radius="10dp"
        />

</shape>